<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN"
	"http://www.seasar.org/dtd/components23.dtd">
<components>

	<component class="org.seasar.chronos.impl.SchedulerImpl"/>
	<component name="jobExecuteContext" class="org.seasar.chronos.task.state.JobExecuteContext"/>
	
	<component class="org.seasar.chronos.task.impl.JobExecutorServiceImpl">
		<aspect>
			<component class="org.seasar.framework.aop.interceptors.TraceInterceptor"/>
		</aspect>
		<property name="jobExecuteStateNonInitialized">jobExecuteStateNonInitialized</property>
		<property name="jobExecuteContext">jobExecuteContext</property>
	</component>
	
	<component name="jobMethodExecuteHandler" class="org.seasar.chronos.task.handler.impl.JobMethodExecuteHandlerImpl">
	</component>
	<component name="jobGroupMethodExecuteHandler" class="org.seasar.chronos.task.handler.impl.JobGroupMethodExecuteHandlerImpl">
		<property name="jobMethodExecuteHandler">jobMethodExecuteHandler</property>
	</component>
	<component name="jobExecuteStrategy" class="org.seasar.chronos.task.strategy.impl.JobExecuteStrategyImpl">
		<property name="jobMethodExecuteHandler">jobMethodExecuteHandler</property>
		<property name="jobGroupMethodExecuteHandler">jobGroupMethodExecuteHandler</property>
	</component>
	
	<component name="jobExecuteStateNonInitialized" class="org.seasar.chronos.task.state.impl.JobExecuteStateNonInitialized">
		<property name="jobExecuteStateInitialized">jobExecuteStateInitialized</property>
		<property name="jobExecuteStrategy">jobExecuteStrategy</property>
	</component>
	<component name="jobExecuteStateInitialized" class="org.seasar.chronos.task.state.impl.JobExecuteStateInitialized">
		<property name="jobExecuteStateNoInitialized">jobExecuteStateNonInitialized</property>
		<property name="jobExecuteStrategy">jobExecuteStrategy</property>
	</component>
	
	
</components>
